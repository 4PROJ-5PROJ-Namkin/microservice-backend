  "info": {
    "title": "Production API Service - Namkin X SUPINFO",
    "description": "<a href=\"/api/v1/production/production-swagger-spec.json\" target=\"_blank\">https://localhost:3002/api/v1/production/production-swagger-spec.json</a>",
