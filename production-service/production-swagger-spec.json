    "contact": {}
  },
  "tags": [],
  "servers": [
    {
      "url": "/api/v1"
    }
  ],
  "components": {
    "schemas": {
      "CreateMaterialDto": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "description": "The name of the material.",
            "example": "zinc"
          }
        },
        "required": [
          "name"
        ]
      },
      "CreateManyMaterialsDto": {
        "type": "object",
        "properties": {
          "materials": {
            "description": "Collection of material objects.",
            "example": [
              {
                "name": "zinc"
              }
            ],
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/CreateMaterialDto"
            }
          }
        },
        "required": [
          "materials"
        ]
      },
      "UpdateMaterialDto": {
        "type": "object",
        "properties": {
          "id": {
            "type": "number",
            "description": "Id to pinpoint the material to update.",
            "example": 12
          },
          "name": {
            "type": "string",
            "description": "The name of the material.",
            "example": "zinc"
          }
        },
        "required": [
          "id",
          "name"
        ]
      },
      "UpdateManyMaterialsDto": {
        "type": "object",
        "properties": {
          "materials": {
            "description": "Array of material update objects.",
            "example": [
              {
                "id": 1,
                "name": "zinc"
              }
            ],
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/UpdateMaterialDto"
            }
          }
        },
        "required": [
          "materials"
        ]
      },
      "MaterialToDeleteDto": {
        "type": "object",
        "properties": {
          "id": {
            "type": "number",
            "description": "Id to pinpoint the material to delete.",
            "example": 12
          }
        },
        "required": [
          "id"
        ]
      },
      "DeleteManyMaterialsDto": {
        "type": "object",
        "properties": {
          "materials": {
            "description": "Array of material delete objects.",
            "example": [
              {
                "id": 1
              }
            ],
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/MaterialToDeleteDto"
            }
          }
        },
        "required": [
          "materials"
        ]
      },
      "CreateMaterialPartInformationsDto": {
        "type": "object",
